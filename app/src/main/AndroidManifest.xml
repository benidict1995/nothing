<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.metromart.nothing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".application.NothingApplication"
        android:theme="@style/Theme.Nothing">
        <activity android:name=".global.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".global.landing.LandingHostActivity"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".global.onboarding.OnboardingActivity"
            android:screenOrientation="portrait"
            />

        <activity
            android:name=".global.login.LoginHostActivity"
            android:screenOrientation="portrait"
            />


        <!-- METRO MART -->

        <!-- METRO FOOD -->

    </application>

</manifest>